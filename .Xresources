URxvt.geometry: 74x27
urxvt.font: xft:DejaVuSansMono:size=12
Urxvt*savelines: 10000
Urxvt*scrollBar: true
URxvt*scrollBar_right:true
URxvt*scrollColor: #c2dd5a
URxvt*scrollBar_floating: true
URxvt*scrollstyle: plain

URxvt.tintColor: #FF0000
URxvt.depth:32
URxvt*background: #101010 
URxvt*foreground: #f2f2f2
URxvt*color0: #101010
URxvt*color1: #f13a21
URxvt*color2: #93f91d
URxvt*color3: #ffd00a
URxvt*color4: #004f9e
URxvt*color5: #ec0048
URxvt*color6: #2aa7e7
URxvt*color7: #f2f2f2
URxvt*color8: #1d202f
URxvt*color9: #ff361e
URxvt*color10: #ffc005
URxvt*color11: #93ff00
URxvt*color12: #0071ff
URxvt*color13: #ef0051
URxvt*color14: #4bb8fd
URxvt*color15: #a020f0

URxvt.keysym.S-Up: \033[1;2A

URxvt.perl-ext-common: default,url-select,resize-font,selection-to-clipboard
URxvt.url-launcher: firefox
URxvt.keysym.M-u: perl: url-slect:select_next
URxvt.url-select.underline: true

URxvt.keysym.C-Down: resize-font:smaller
URxvt.keysym.C-Up : resize-font:bigger


# Config for hight definition dpi (https://wiki.archlinux.org/index.php/HiDPI#X_Server)

Xft.dpi: 180
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
